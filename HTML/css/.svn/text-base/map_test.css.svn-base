/*小地點*/
.place_listbox{
	overflow: auto;
	height: 262px;
	width: 118px;
	padding: 5px;
	visibility: visible;
	text-align: left;
	font-size: 12px;
	color: #333333;
}
.place_list:link,.place_list:visited,.place_list:active{
	font-size: 14px;
	color: #333333;
	text-decoration: none;
}
.place_list:hover{
	font-size: 14px;
	color: #990000;
	text-decoration: none;
}
/*戰鬥紐*/
.map_btn_fight:link,.map_btn_fight:visited,.map_btn_fight:active {
	display:block;
	background-image: url(../images/map_btn_fight.gif);
	background-repeat: no-repeat;
	background-position: center;
	height: 28px;
}
.map_btn_fight:hover{
	background-image: url(../images/map_btn_fight_over.gif);
	background-repeat: no-repeat;
	background-position: center;
}
