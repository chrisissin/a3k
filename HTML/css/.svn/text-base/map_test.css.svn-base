/*目前位於*/
.map li.location{
	background-image: url(../images/type_5_27.gif);
	background-position: top;
	height: 30px;
	width: 227px;
	text-align: center;
	display: block;
	line-height: 30px;
	margin: 0px;
	padding: 0px;
	list-style-type: none;
	left: 0px;
	top: 0px;
	z-index: 3;
}
.location_text {
	font-size: 14px;
	color: #666666;
}
/*大地點*/
.location_text2 {
	font-size: 18px;
	font-weight: bold;
	color: #009900;
}
/*地圖所在州*/
.location_text3 {
	font-size: 18px;
	font-weight: bold;
	color: #0066FF;
}
