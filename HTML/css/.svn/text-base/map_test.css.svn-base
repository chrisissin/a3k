/*地圖*/
.map {
	list-style: none;
	width: 465px;
	height: 360px;
	position: relative;
	margin: 0;
	padding: 0;
	background-image: url(../images/map.jpg);
	background-repeat: no-repeat;
	background-position: left top;
	z-index: 1;
}
.map li {
	list-style: none;
	margin: 0;
	position: absolute;
	font-size: 12px;
	color: #000000;
	text-decoration: none;
}
