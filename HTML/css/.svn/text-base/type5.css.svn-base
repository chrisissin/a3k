@charset "utf-8";
/*index.html*//*左排背景*/
.l_bg {
	background-image: url(../images/type_5_15.gif);
	border-right-width: 2px;
	border-bottom-width: 2px;
	border-right-style: solid;
	border-bottom-style: solid;
	border-right-color: #CCCCCC;
	border-bottom-color: #CCCCCC;
	width: 176px;
}
/*對話打字*/
.talk {
	height: 18px;
	width: 434px;
	font-size: 11px;
	color: #000000;
	display: block;
	border: 1px solid #663300;
	text-decoration: none;
	line-height: 20px;
}
/*「全頻」*/
.talk_select {
	font-size: 11px;
	color: #663300;
	text-decoration: none;
	display: block;
	border: 1px solid #663300;
}
/*對話顯示框*/
.talkbox {
	font-size: 11px;
	color: #663300;
	margin: 3px;
	height: 99%;
	width: 99%;
	overflow: auto;
}
/*主按鈕*/
.btn_top:link,.btn_top:visited,.btn_top:active {
	font-size: 16px;
	font-weight: bold;
	color: #000000;
	text-decoration: none;
	background-image: url(../images/btn_top.gif);
	background-repeat: no-repeat;
	background-position: center center;
	display: block;
	height: 39px;
	width: 90px;
	text-align: center;
	line-height: 40px;
}
.btn_top:hover{
	font-size: 16px;
	font-weight: bold;
	color: #663300;
	text-decoration: none;
	background-image: url(../images/btn_top_over.gif);
	background-repeat: no-repeat;
	background-position: center center;
	text-align: center;
	vertical-align: middle;
	display: block;
	height: 39px;
	width: 90px;
	line-height: 40px;
}
/*任務*/
.mission {
	font-size: 11px;
	color: #336600;
	text-decoration: none;
	height: 99%;
	width: 99%;
	overflow: auto;
	padding-top: 3px;
	padding-left: 3px;
	padding-right: 3px;
	padding-bottom: 0px;
}
/*公告*/
.news:link,.news:visited,.news:active{
	font-size: 12px;
	line-height: 25px;
	color: #FF0000;
	text-decoration: none;
	height: 99%;
	width: 99%;
	padding-left: 5px;
}
.news:hover {
	font-size: 12px;
	line-height: 25px;
	color: #FF0000;
	text-decoration: none;
	height: 99%;
	width: 99%;
}
/*「傳令」*/
.news_title {
	font-size: 12px;
	color: #000000;
	background-image: url(../images/type_5_25.gif);
	text-align: center;
	vertical-align: middle;
	height: 26px;
	width: 67px;
	line-height: 25px;
	letter-spacing: 2pt;
	font-weight: normal;
	background-repeat: no-repeat;
	background-position: right;
}


/*右上角按鈕「幫助、商城」*/
.btn_ru:link,.btn_ru:visited,.btn_ru:active {
	display:block;
	background-image: url(../images/btn_ru.gif);
	background-repeat: no-repeat;
	background-position: center center;
	font-size: 12px;
	color: #FFFFFF;
	text-decoration: none;
	height: 76px;
	width: 57px;
	text-align: center;
	font-family: "新細明體", Garamond;
	line-height: 58px;
}
.btn_ru:hover {
	display:block;
	font-size: 12px;
	color: #FFFFFF;
	text-decoration: none;
	background-image: url(../images/btn_ru_over.gif);
	height: 77px;
	width: 59px;
	background-repeat: no-repeat;
	background-position: right center;
	font-family: "新細明體", Garamond;
	line-height: 58px;
}


/*下排按鈕*/
.btn_down:link,.btn_down:visited,.btn_down:active {
	background-image: url(../images/btn_down.gif);
	font-size: 11px;
	color: #663300;
	text-decoration: none;
	height: 20px;
	width: 36px;
	display: block;
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: solid;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-top-color: #663300;
	border-right-color: #663300;
	border-bottom-color: #663300;
	border-left-color: #663300;
	text-align: center;
	vertical-align: middle;
	line-height: 20px;
}
.btn_down:hover {
	font-size: 11px;
	color: #000000;
	text-decoration: none;
	background-image: url(../images/btn_down_over.gif);
	height: 20px;
	width: 36px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-right-color: #663300;
	border-bottom-color: #663300;
	border-left-color: #663300;
	display: block;
	vertical-align: sub;
	border-top-style: none;
	line-height: 18px;
}


/*主畫面*/
.main {
	height: 362px;
	width: 762px;
	border-right-width: 2px;
