  PRIMARY KEY  USING BTREE (`p_id`),
  KEY `p_owner` (`p_owner`),
  KEY `role_id` (`role_id`)
) ENGINE=InnoDB AUTO_INCREMENT=75 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='玩家的武將';

